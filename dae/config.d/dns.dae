# Use alidns for China mainland domains and googledns for others.
dns {
  upstream {
    mosdns: "udp://127.0.0.1:5335"
  }
  routing {
    # According to the request of dns query, decide to use which DNS upstream.
    # Match rules from top to bottom.
    request {
      fallback: mosdns
    }
  }
}
